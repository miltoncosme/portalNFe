		<script>
			var $listaNFe = []
			function buscaNFe() {
				const emp = document.getElementById("form-field-select-1");
				const p = document.getElementById("form-field-select-2");
				const cnpj = emp.options[emp.selectedIndex].value;
				if (cnpj.length!=14){
					confirm("Selecione uma empresa!");
					return;
				}
				let mes = p.options[p.selectedIndex].value;
				let ano = String(p.options[p.selectedIndex].text.split('/')[1]);
				if (ano.length!=4){
					confirm("Selecione o periodo desejado!");
					return;
				}
				
				$.ajax({	url: `/server/nfe/cnpj/${cnpj}/mesano/${mes}.${ano}`,
							type: "get",
							data: {}
				})
					.done(function (result) {
						Table.clear().draw();
						Table.rows.add(result.dados).draw();								
					})
					.fail(function (jqXHR, textStatus, errorThrown) {
						// needs to implement if it fails
						
					});
			}
			function baixarNFe(){
				const emp = document.getElementById("form-field-select-1");
				const p = document.getElementById("form-field-select-2");
				const cnpj = emp.options[emp.selectedIndex].value;
				let mes = p.options[p.selectedIndex].value;
				let ano = String(p.options[p.selectedIndex].text.split('/')[1]);			
				let dados = document.getElementById('dadosTabela');
				let icount = dados.getElementsByTagName('td').length;	
				if (cnpj.length!=14){
					confirm("Selecione uma empresa!");
					return;
				} else if (ano.length!=4){
					confirm("Selecione o periodo desejado!");
					return;
				} else if (icount in [0,1]){
					confirm('Sua sele√ß√£o nao retornou nenhuma busca!')
					return;
				};
				
				$.ajax({	url: `/server/download/nfe/cnpj/${cnpj}/mesano/${mes}.${ano}`,
							type: "get",
							data: {}
				})				
					.done(function (res) {						
						var classDiv = document.createAttribute("class");
						classDiv.value='alert alert-info';
						var div = document.createElement("div");
						div.setAttributeNode(classDiv);
						//
						var classI = document.createAttribute("class");
						classI.value = 'ace-icon fa fa-times';
						var i = document.createElement("i");
						i.setAttributeNode(classI);
						//						
						var classButton = document.createAttribute("class");
						classButton.value = 'close';						
						var datadismissButton = document.createAttribute("data-dismiss");
						datadismissButton.value = 'alert';
						var button = document.createElement("button");
						button.setAttributeNode(classButton);
						button.setAttributeNode(datadismissButton);
						//
						button.appendChild(i);
						div.appendChild(button);
						//
						
						var a = document.createElement("a");
						var h1 = document.createElement("label");
						var p1 = document.createTextNode("Click em ");
						var p2 = document.createTextNode(" para baixar o seu arquivo.");
						var href = document.createAttribute("href"); 
						var download = document.createAttribute("download"); 
						download.value = `NFe_${p.options[p.selectedIndex].text}.zip`;
						var textnode = document.createTextNode("AQUI");    
						href.value = res.dados.uri;
						a.setAttributeNode(href);
						a.setAttributeNode(download);
						a.appendChild(textnode);
						h1.appendChild(p1);
						h1.appendChild(a);
						h1.appendChild(p2);
						div.appendChild(h1);
						document.getElementById("download").appendChild(div);
					})					
					.fail(function (jqXHR, textStatus, errorThrown) {
						// needs to implement if it fails
						confirm(jqXHR.responseText);
					});
			}

			function limpaDados(){
				var table = $('#dynamic-table').DataTable();				
				table
					.clear()
					.draw();
				const myNode = document.getElementById("download");
				while (myNode.firstChild) {
					myNode.removeChild(myNode.firstChild);
				}		
			}
		</script>
